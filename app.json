{
  "expo": {
    "name": "nutrition",
    "slug": "nutrition",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/food-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/food-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kim_enteria_rl2001.nutrition"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/food-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/food-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1701dce9-1d3c-4ffe-9126-933de2532e89"
      }
    },
    "owner": "kim_enteria_rl2001"
  }
}
